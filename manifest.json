{
    "manifest_version": 2,
    "name": "Tab Recorder",
    "description": "Tab Recorder",
    "version": "0.0.1",
    "browser_action": {
        "default_title": "Tab Recorder"
    },
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "permissions": [
        "tabCapture"
    ]
}
